{
    "extends": "airbnb-base",
    "env": {
        "node": true,
        "mocha": true
    },
    "rules": {
        "comma-dangle": [2, "never"],
        "indent": ["error", 4],
        "no-underscore-dangle": 0,
        "no-param-reassign": 0,
        "class-methods-use-this": 0,
        "max-len": ["error", 160],
        "arrow-parens": 0,
        "arrow-body-style": 0,
        "camelcase": 0
    },
    "settings": {
    },
    "plugins": [],
    "globals": {}
}
